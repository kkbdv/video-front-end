/* pages/form/form.wxss */
.cardContent{
  width:100%;
  height:100px;background:#FFFFFF;
  display: flex;
  flex-direction:row;
  justify-content: space-around;
  align-items: center;
}
/* li{
  list-style-type: none
} */
.cardContent .detail{
  width:50%;height:83%;background:#FFFFFF;font-size:13px;line-height: 22px;
}

.blockStart{
  display: flex;flex-direction:column;margin:5px 5px
}
.status{
  width:100%;height:20px;background:#F5F5F5;border-radius:5px 5px 0 0;display: flex;flex-direction:row;
}
.status .dot{
  height:10px;width:10px;border-radius:50%;margin:5px 5px
}

.tips{
  width:100%;height:15px;border-top:1px solid #D3D3D3;background:#F5F5F5;border-radius:0 0 5px 5px
}

.tips text{
  font-size:10px;float:right;margin-right:12px
}